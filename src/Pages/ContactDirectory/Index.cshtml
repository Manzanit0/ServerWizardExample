@page
@model ServerWizardExample.Pages.ContactDirectory.Index

@{
    ViewData["Title"] = "Contacts";
}

<!DOCTYPE html>

<html>
<head>
    <title></title>
</head>
<body>
<table class="table">
    <thead>
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Email
        </th>
        <th>
           Phone 
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Contacts)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
        </tr>
    }
    </tbody>
</table>
</body>
</html>